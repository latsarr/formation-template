      // activation de la compilation en cascade
      "objectscript.compileFlags": "cukb",
    "objectscript.conn" :{
      "ns": "IRISAPP",
      "username": "SuperUser",
      "password": "SYS",
      "host": "localhost",
      "port": 52775,
      "active": true,
    },
